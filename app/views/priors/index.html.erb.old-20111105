<h1>Listing priors</h1>
<table>
  <tr>
    <th>Search by Variable</th>
  </tr>
  <tr>
    <td>
      <%= text_field_tag :search %><span id="spinner" style="display: none;width: 16px"><img src="data:image/gif;base64," /></span>
    </td>
  </tr>
</table>

<div id="search_results"></div>

<table>
  <tr>
    <th>Citation</th>
    <th>Variable</th>
    <th>(units)</th>
    <th>Phylogeny</th>
    <th>Distribution</th>
    <th>Parameter a</th>
    <th>Parameter b</th>
    <th>N</th>
    <th>Notes</th>
  </tr>

<% @priors.each do |prior| %>
  <tr>
    <td><%= prior.citation.author_year_title if !prior.citation.nil? %></td>
    <td><%= link_to prior.variable.name, variable_path(prior.variable) rescue "" %></td>
    <td><%= prior.variable.units rescue "" %></td>
    <td><%= prior.phylogeny %></td>
    <td><%= prior.distn %></td>
    <td><%= prior.parama %></td>
    <td><%= prior.paramb %></td>
    <td><%= prior.n %></td>
    <td><%= prior.notes %></td>
    <td><%= link_to 'Show', prior %></td>
    <td><%= link_to 'Edit', edit_prior_path(prior) %></td>
    <td><%= link_to 'Destroy', prior, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @priors %>

<br />

<%= link_to 'New prior', new_prior_path %>

<%= observe_field(:search,
                  :frequency => 1,
                  :with => 'symbol',
                  :loading => "Element.show('spinner')",
                  :complete => "Element.hide('spinner')",
                  :url => { :action => :search }) %>
