      <td><%= image_tag("redcheck.png", :id => "ok#{@count}") %> <%= observe_field("trait_#{@count}_mean", :frequency => 1, :url => { :action => "check_trait"}, :with => "'count=#{@count}&trait[site_id]='+$('trait_#{@count}_site_id').value+'&trait[cultivar_id]='+$('trait_#{@count}_cultivar_id').value+'&trait[treatment_id]='+$('trait_#{@count}_treatment_id').value+'&trait[date(1i)]='+$('trait_#{@count}_date_1i').value+'&trait[date(2i)]='+$('trait_#{@count}_date_2i').value+'&trait[date(3i)]='+$('trait_#{@count}_date_3i').value+'&trait[dateloc]='+$('trait_#{@count}_dateloc').value+'&trait[time(4i)]='+$('trait_#{@count}_time_4i').value+'&trait[time(5i)]='+$('trait_#{@count}_time_5i').value+'&trait[timeloc]='+$('trait_#{@count}_timeloc').value+'&trait[variable_id]='+$('trait_#{@count}_variable_id').value+'&trait[mean]='+$('trait_#{@count}_mean').value+'&trait[n]='+$('trait_#{@count}_n').value+'&trait[statname]='+$('trait_#{@count}_statname').value+'&trait[stat]='+$('trait_#{@count}_stat').value+'&trait[access_level]='+$('trait_#{@count}_access_level').value")  %>
