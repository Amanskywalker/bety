<script>
function reset() {
  for(var i = 0; i <layers.length;i++){
    layers[i].setMap(null)
  }
}

function updatemap() {
  select = document.getElementById('selectmap')
  reset();
  layers[select.options[select.selectedIndex].value].setMap(map);

}
</script>


